{
  "name": "code-splitting-practice",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "author": "addhome2001",
  "scripts": {
    "start": "webpack-dev-server --env=dev --profile --colors --progress",
    "clear": "rimraf dist",
    "lint": "./node_modules/.bin/eslint . --ignore-path .gitignore",
    "debug": "better-npm-run debug",
    "build": "npm run clear && webpack --env=prod --progress --profile --colors",
    "build:prod": "better-npm-run build:prod",
    "build:demo": "better-npm-run build:demo"
  },
  "betterScripts": {
    "debug": {
      "command": "npm start",
      "env": {
        "DEBUG": "*"
      }
    },
    "build:prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PUBLIC_PATH": "/"
      }
    },
    "build:demo": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PUBLIC_PATH": "/code-splitting-practice"
      }
    }
  },
  "engines": {
    "node": ">=6.2.2",
    "npm": ">=3.9.5"
  },
  "license": "ISC",
  "dependencies": {
    "capitalize": "^1.0.0",
    "chunk-manifest-webpack2-plugin": "^1.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "inline-chunk-manifest-html-webpack-plugin": "^0.1.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-avatar": "^2.3.0",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.2",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-md5-hash": "^0.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "better-npm-run": "^0.0.15",
    "css-loader": "^0.26.4",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2"
  }
}
